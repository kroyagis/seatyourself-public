<!DOCTYPE html>
<html>
<head>
  <title>Seatyourself</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>


  <% if current_user %>
    <%= link_to "Log Out", session_path("current"), method: :delete  %><br>
    <%= link_to "View Profile", user_path(current_user.id) %>
  <% else %>
    <%= link_to "Sign Up", new_user_path %><br>
    <%= link_to "Log In", new_session_path %><br>
  <% end %>



<%if flash[:notice]%>
<p id="notice"><%= flash[:notice] %></p>
<% end %>

<% if flash[:alert]%>
<p id="alert"><%= flash[:alert] %></p>
<% end %>

<%= yield %>

<%= console %>

</body>
</html>
